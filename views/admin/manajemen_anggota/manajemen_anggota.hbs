<div class="flex flex-col rounded-none max-w-[965px]" role="region" aria-label="Member Management Section">
  <div class="flex flex-wrap gap-5 justify-between w-full max-md:max-w-full">
    <h1 class="text-4xl font-extrabold leading-none text-black">
      Manajemen Anggota
    </h1>
    <a class="flex items-start self-start mt-3 text-sm font-medium leading-none text-white rounded-lg" aria-label="Add Member" 
    href="/admin/manajemen_anggota/tambah_anggota">
      <div class="flex overflow-hidden gap-2 justify-center items-center px-5 py-3 bg-blue-800 rounded-lg border border-gray-300 border-solid shadow-sm">
        <img
          loading="lazy"
          src="https://cdn.builder.io/api/v1/image/assets/TEMP/c8ee5b9060d0d9aa40d7492b4076d5391cf96b9becf4c994b7983a3adfe73c1c?placeholderIfAbsent=true&apiKey=0eb9abb28bf34cd7be47a2dfb2f311cb"
          alt=""
          class="object-contain shrink-0 self-stretch my-auto aspect-[0.93] w-[13px]"
        />
        <span>Tambah Anggota</span>
      </div>
    </a>
  </div>

  <div class="flex flex-wrap gap-5 justify-between mt-2 w-full whitespace-nowrap max-md:max-w-full">
    <form class="flex flex-col text-base leading-normal text-gray-500" role="search">
      <div class="flex flex-col w-full">
        <label for="searchMembers" class="sr-only">Search Members</label>
        <div class="flex gap-2 items-center px-3.5 py-2.5 w-full bg-white rounded-lg border-gray-300 border-solid">
          <div class="flex flex-1 shrink gap-2 items-center self-stretch my-auto w-full basis-0 min-w-[240px]">
            <img
              loading="lazy"
              src="https://cdn.builder.io/api/v1/image/assets/TEMP/c3298dfdfad3de5332e45e084e12d2172f36afb3b78a8a3653d0a71b92ee77ff?placeholderIfAbsent=true&apiKey=0eb9abb28bf34cd7be47a2dfb2f311cb"
              alt=""
              class="object-contain shrink-0 self-stretch my-auto aspect-square w-[18px]"
            />
            <input type="search" id="searchMembers" placeholder="Search" class="w-full bg-transparent border-none rounded-lg focus:outline-none" />
          </div>
        </div>
      </div>
    </form>

    <button class="flex items-start text-sm mt-6 font-medium leading-none rounded-lg min-h-[40px] text-slate-700" aria-label="Filter Members">
      <div class="flex overflow-hidden gap-2 justify-center items-center px-4 py-2.5 bg-white rounded-lg border border-gray-300 border-solid shadow-sm">
        <img
          loading="lazy"
          src="https://cdn.builder.io/api/v1/image/assets/TEMP/565ef0f7d60ee4cafb3e62c36e3f2b725be54df8b71e3b1813ba0884d857d57b?placeholderIfAbsent=true&apiKey=0eb9abb28bf34cd7be47a2dfb2f311cb"
          alt=""
          class="object-contain shrink-0 self-stretch my-auto w-5 aspect-square"
        />
        <span>Filters</span>
      </div>
    </button>
  </div>

</div>
<!-- Table Section -->
<div class="max-w-[85rem] px-4 py-10 sm:px-6 lg:px-8 lg:py-2 mx-auto">
  <!-- Card -->
  <div class="flex flex-col">
    <div class="-m-1.5 overflow-x-auto">
      <div class="p-1.5 min-w-full inline-block align-middle">
        <div class="bg-white border border-gray-200 rounded-xl shadow-sm overflow-hidden">

          <!-- Table -->
          <table class="min-w-full divide-y divide-gray-200">
            <thead class="bg-gray-100">
              <tr>
                <th scope="col" class="px-6 py-4 text-start">
                  <div class="flex items-center gap-x-2">
                    <span class="text-xs font-semibold uppercase tracking-wide text-gray-800">
                      NO
                    </span>
                    
                  </div>
                </th>

                <th scope="col" class="px-6 py-3 text-start">
                  <div class="flex items-center gap-x-2">
                    <span class="text-xs font-semibold uppercase tracking-wide text-gray-800">
                      NAMA/NIP
                    </span>
                  </div>
                </th>

                <th scope="col" class="px-6 py-3 text-start">
                  <div class="flex items-center gap-x-2">
                    <span class="text-xs font-semibold uppercase tracking-wide text-gray-800">
                      JABATAN
                    </span>
                  </div>
                </th>

                <th scope="col" class="px-6 py-3 text-start">
                  <div class="flex items-center gap-x-2">
                    <span class="text-xs font-semibold uppercase tracking-wide text-gray-800">
                      BIDANG
                    </span>
                  </div>
                </th>

                <th scope="col" class="px-6 py-3 text-start">
                  <div class="flex items-center gap-x-2">
                    <span class="text-xs font-semibold uppercase tracking-wide text-gray-800">
                      STATUS
                    </span>
                  </div>
                </th>

                <th scope="col" class="px-6 py-3 text-start">
                  <div class="flex items-center gap-x-2">
                    <span class="text-xs font-semibold uppercase tracking-wide text-gray-800">
                      OPSI
                    </span>
                  </div>
                </th>

                <th scope="col" class="px-6 py-3 text-end"></th>
              </tr>
            </thead>

            <tbody class="divide-y divide-gray-200">
              {{#each dataAnggota }}
                <tr class="bg-white hover:bg-gray-50">
                  <td class="size-px whitespace-nowrap">
                    <button type="button" class="block" aria-haspopup="dialog" aria-expanded="false" aria-controls="hs-ai-invoice-modal" data-hs-overlay="#hs-ai-invoice-modal">
                      <span class="block px-6 py-2">
                        <span class="font-mono text-sm text-blue-600">{{inc @index}}</span>
                      </span>
                    </button>
                  </td>
                  <td class="size-px whitespace-nowrap">
                    <button type="button" class="block text-start" aria-haspopup="dialog" aria-expanded="false" aria-controls="hs-ai-invoice-modal" data-hs-overlay="#hs-ai-invoice-modal">
                      <span class="block px-6 py-2">
                        <p class="text-sm font-bold text-gray-600">{{nama}}</p>
                        <p class="text-sm text-gray-500">{{nip}}</p>
                      </span>
                    </button>
                  </td>

                  <td class="size-px whitespace-nowrap">
                    <button type="button" class="block text-start" aria-haspopup="dialog" aria-expanded="false" aria-controls="hs-ai-invoice-modal" data-hs-overlay="#hs-ai-invoice-modal">
                      <span class="block px-6 py-2">
                        <span class="text-sm font-bold text-gray-600">{{jabatan}} </span>
                      </span>
                    </button>
                  </td>

                  <td class="size-px whitespace-nowrap">
                    <button type="button" class="block text-start" aria-haspopup="dialog" aria-expanded="false" aria-controls="hs-ai-invoice-modal" data-hs-overlay="#hs-ai-invoice-modal">
                      <span class="block px-6 py-2">
                        <span class="text-sm font-bold text-gray-600">{{tim}}</span>
                      </span>
                    </button>
                  </td>

                  <td class="size-px whitespace-nowrap">
                    <button type="button" class="block" aria-haspopup="dialog" aria-expanded="false" aria-controls="hs-ai-invoice-modal" data-hs-overlay="#hs-ai-invoice-modal">
                      <span class="block px-6 py-2">
                        {{#if status}}
                          <span class="py-1 px-1.5 inline-flex items-center gap-x-1 text-xs font-medium bg-teal-100 text-teal-800 rounded-full">
                            <svg class="size-2.5" xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" viewBox="0 0 16 16">
                              <path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-3.97-3.03a.75.75 0 0 0-1.08.022L7.477 9.417 5.384 7.323a.75.75 0 0 0-1.06 1.06L6.97 11.03a.75.75 0 0 0 1.079-.02l3.992-4.99a.75.75 0 0 0-.01-1.05z" />
                            </svg>
                            Aktif
                          </span>
                        {{else}}
                          <span class="inline-flex items-center gap-1.5 py-0.5 px-2 rounded-full text-xs font-medium bg-red-100 text-red-800">
                            <svg class="size-2.5" xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" viewBox="0 0 16 16">
                              <path
                                d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zM5.354 4.646a.5.5 0 1 0-.708.708L7.293 8l-2.647 2.646a.5.5 0 0 0 .708.708L8 8.707l2.646 2.647a.5.5 0 0 0 .708-.708L8.707 8l2.647-2.646a.5.5 0 0 0-.708-.708L8 7.293 5.354 4.646z" />
                            </svg>
                            Tidak Aktif
                          </span>
                        {{/if}}
                        
                      </span>
                    </button>
                  </td>

                  <td class="flex space-x-2 pt-4">
                    <a href="/admin/manajemen_anggota/detail_anggota" class="block">
                      <img
                        loading="lazy"
                        src="https://cdn.builder.io/api/v1/image/assets/TEMP/7df477c4909d1d81c2a6c98f4dc2661eafcc7ec13a1bad8ec688f48f8d17b695?apiKey=0eb9abb28bf34cd7be47a2dfb2f311cb&"
                        class="object-contain shrink-0 rounded-none aspect-square w-[25px]"
                        alt="detail anggota"
                      />
                    </a>
                    <a href="/admin/manajemen_anggota/edit_anggota" class="block">
                      <img
                        loading="lazy"
                        src="https://cdn.builder.io/api/v1/image/assets/TEMP/0fe336745368fc2896cc5f48a0a41e930b8a14cd2f6ec3a94d596ab121911229?apiKey=0eb9abb28bf34cd7be47a2dfb2f311cb&"
                        class="object-contain shrink-0 rounded-none aspect-square w-[25px]"
                        alt="edit anggota"
                      />
                    </a>
                    <a href="/admin/manajemen_anggota/ubahpw_anggota" class="block">
                      <img
                        loading="lazy"
                        src="https://cdn.builder.io/api/v1/image/assets/TEMP/8f06f664e2fb7d1c726d403a71395d07e5c742fee0b1c8fd82897465315e91c9?apiKey=0eb9abb28bf34cd7be47a2dfb2f311cb&"
                        class="object-contain shrink-0 rounded-none aspect-square w-[25px]"
                        alt="ubah password anggota"
                      />
                    </a>
                    <a href="/admin/manajemen_anggota/hapus_anggota" class="block">
                      <img
                        loading="lazy"
                        src="https://cdn.builder.io/api/v1/image/assets/TEMP/4b2b586f44984b67bb7814a0bec30a83ec480b698578cd88e4f7cff46da40007?apiKey=0eb9abb28bf34cd7be47a2dfb2f311cb&"
                        class="object-contain shrink-0 rounded-none aspect-square w-[25px]"
                        alt="hapus anggota"
                      />
                    </a>
                  </td>

                </tr>
              {{/each}}

            </tbody>
          </table>
          <!-- End Table -->

          <!-- Footer -->
          <div class="px-6 py-4 grid gap-3 md:flex md:justify-between md:items-center border-t border-gray-200">
            <div>
              <p class="text-sm text-gray-600">
                <span class="font-semibold text-gray-800">9</span>
                results
              </p>
            </div>

            <div>
              <div class="inline-flex gap-x-2">
                <button
                  type="button"
                  class="py-2 px-3 inline-flex items-center gap-x-2 text-sm font-medium rounded-lg border border-gray-200 bg-white text-gray-800 shadow-sm hover:bg-gray-50 focus:outline-none focus:bg-gray-50 disabled:opacity-50 disabled:pointer-events-none"
                >
                  <svg class="size-3" width="16" height="16" viewBox="0 0 16 15" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path d="M10.506 1.64001L4.85953 7.28646C4.66427 7.48172 4.66427 7.79831 4.85953 7.99357L10.506 13.64" stroke="currentColor" stroke-width="2" stroke-linecap="round" />
                  </svg>
                  Prev
                </button>

                <button
                  type="button"
                  class="py-2 px-3 inline-flex items-center gap-x-2 text-sm font-medium rounded-lg border border-gray-200 bg-white text-gray-800 shadow-sm hover:bg-gray-50 focus:outline-none focus:bg-gray-50 disabled:opacity-50 disabled:pointer-events-none"
                >
                  Next
                  <svg class="size-3" width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path d="M4.50598 2L10.1524 7.64645C10.3477 7.84171 10.3477 8.15829 10.1524 8.35355L4.50598 14" stroke="currentColor" stroke-width="2" stroke-linecap="round" />
                  </svg>
                </button>
              </div>
            </div>
          </div>
          <!-- End Footer -->
        </div>
      </div>
    </div>
  </div>
  <!-- End Card -->
</div>
<!-- End Table Section -->

<!-- Modal -->
<div id="hs-ai-invoice-modal" class="hs-overlay hidden size-full fixed inset-0 z-[80] overflow-x-hidden overflow-y-auto pointer-events-auto" role="dialog" tabindex="-1" aria-labelledby="delete-member-title">
  <div class="hs-overlay-open:mt-7 hs-overlay-open:opacity-100 hs-overlay-open:duration-500 mt-0 opacity-0 ease-out transition-all sm:max-w-xl sm:w-full m-3 sm:mx-auto">
    <div class="flex flex-col px-10 p-8 bg-white rounded-xl shadow-lg max-w-2xl mx-auto">
      <!-- Header -->
      <div class="flex items-center gap-6">
        <img src="https://cdn.builder.io/api/v1/image/assets/TEMP/c5f1394b0798b1548cd832008e9a74fe0d7e3eb3f4bea406d715b2e5c3842fe5?placeholderIfAbsent=true&apiKey=0eb9abb28bf34cd7be47a2dfb2f311cb" alt="Warning Icon" class="w-16 h-16" />
        <div>
          <h2 id="delete-member-title" class="text-2xl text-black">
            Hapus
            <b>Budi Santoso</b>?
          </h2>
          <p class="mt-2 text-sm text-gray-600">Anggota akan terhapus secara permanen</p>
        </div>
      </div>

      <!-- Footer Buttons -->
      <div class="flex justify-end gap-4 mt-8">
        <a href="" class="px-6 mb-4 flex items-center justify-center text-black border border-black rounded-lg hover:bg-gray-100 focus:outline-none">
          Batal
        </a>
        <form method="POST" action="hapusanggota">
          <button type="submit" class="px-6 py-2 text-white bg-black rounded-lg hover:bg-gray-800 focus:outline-none">
            Hapus
          </button>
        </form>
      </div>
    </div>
  </div>
</div>
<!-- End Modal -->
